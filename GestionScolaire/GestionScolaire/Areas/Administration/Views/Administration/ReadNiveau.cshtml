@using GestionScolaire.Models
@model NiveauModels

@{
    ViewBag.Title = @Model.title;
}
<h2>Niveaux @Model.title</h2>

<table class="table">

    <tr>
        <th>Id</th>
        <th>Titre</th>
        <th>Cycle</th>
    </tr> 
    <tr>
        <td>@Model.id</td>
        <td>@Model.title</td>
        <td>@Html.ActionLink(Model.cycleTitle, "ReadCycle", "Administration", new {area = "Administration",id = Model.cycleId}, new { target = "_blank" })</td>
    </tr>
   
</table>
