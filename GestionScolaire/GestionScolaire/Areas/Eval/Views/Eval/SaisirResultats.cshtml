@using GestionScolaire.Areas.Eval.Models
@model EvaluationModels

@{
    ViewBag.Title = "Saisie des résultats";
}

@using (GestionScolaire.Areas.Eleves.Models.EleveRepository repository = new GestionScolaire.Areas.Eleves.Models.EleveRepository())
{
    foreach (var eleve in repository.All())
    {
        <p>@eleve.LastName</p>
    }
}

@foreach (var eleve in Model.eleves)
{
     <p>@eleve.lastName</p><input type="text" value="" />
}

